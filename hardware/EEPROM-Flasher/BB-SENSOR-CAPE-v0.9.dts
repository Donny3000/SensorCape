/dts-v1/;
/plugin/;

/ {
    compatible = "ti,beaglebone", "ti,beaglebone-black";

    /* identification */
    part-number = "BB-SENSOR-CAPE-v0.9";

    /* version */
    version = "00A0";

    /* State the resources this cape uses  */
    fragment@0 {
        target = <&i2c2>;
        __overlay__ {
            status = "okay";
        };
    };

};
